%h1.my-5= link_to 'Dashboard', dashboard_path

.row
  .col
    %h2 Users
.row.mt-5
  .col
    %ul.list-group.nobullets
      - @users.each do |user|
        %li.mb-2.d-flex.justify-content-between.align-items-center.h4[user]
          = label user.email, user, class: 'list-group-item list-group-item-action'

